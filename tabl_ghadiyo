#include<iostream>
using namespace std;
int main()
{

    int i,j,n;
    char ch;
 value:
    cout<<"Enter value";
    cin>>n;

    for(i=1;i<=10;i++)
    {
        cout<<n<<"*"<<i<<"="<<n*i<<endl;
    }
    cout<<"Do you want to continue?";
    cin>>ch;

    if(ch == 'y' || ch == 'Y')
       {
           goto value;

       }
    else
       {
           cout<<"Bye Love you Bro";
       }

}
